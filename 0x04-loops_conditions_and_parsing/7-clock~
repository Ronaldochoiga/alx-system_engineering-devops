#!/usr/bin/env bash
# script that shows time in 12 hrs
# and 59 min
HOUR=0
while [ $HOUR -le 12 ];
    do
        echo "Hour: $HOUR"
        MIN=1
        while [ $MIN -le 59 ];
        do
            echo "$MIN"
            (( MIN++))
        done
        (( HOUR++))
done
